<section>

    <div class="product-list">
        {{#each cartFilters}}

        <div class="products">
            <a href="/products/addCart/{{this.id}}?method=delete">Delete</a>
            <a href="/products/productDetail/{{this.id}}">
                <div class="movieTitle">{{this.name}} **Buy:{{this.price}} Rent:{{this.rent}}**</div>
                <img src="/uploads/{{this.sPic}}" alt="pic">
            </a>
        </div>

        {{/each}}
    </div>


</section>